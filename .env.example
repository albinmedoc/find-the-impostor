UMAMI_SCRIPT_URL=https://umami.example.com/umami.js
UMAMI_WEBSITE_ID=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# ========================================
# AI Provider Configuration
# ========================================

# Provider type: openrouter, openai, anthropic, or gemini
# Defaults to openrouter if not set
AI_PROVIDER=openrouter

# API Key for the selected provider
AI_API_KEY=your_api_key_here

# (Optional) Base URL - defaults to provider's standard URL if not set
# AI_BASE_URL=https://openrouter.ai/api/v1

# Model to use
AI_MODEL=google/gemini-2.0-flash-exp:free

# Next.js Public URL (for OpenRouter)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Sentry DSN for error tracking and performance monitoring
# Server-side and Edge runtime
SENTRY_DSN=https://your-sentry-dsn@your-sentry-instance.ingest.sentry.io/your-project-id
# Client-side (requires NEXT_PUBLIC_ prefix)
NEXT_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@your-sentry-instance.ingest.sentry.io/your-project-id
